s={props.buttonClass}\r\n                    setShowPopUp={setShowPopUp}/>\r\n                    :null}\r\n                    {props.FbButton ?\r\n                    <div className='logSign__facebookButton'>\r\n                        <span className='logSign__divide'>OR</span>\r\n                        <FbButtion/>\r\n                    </div>\r\n                    : null\r\n                    }\r\n                    {props.endText ? \r\n                        <TextBlock text={props.endText}/>\r\n                    : null}\r\n                    {props.buttonText ?\r\n                    <Button buttonText={props.buttonText} onClick={props.onClick} buttonClass={props.buttonClass}/>\r\n                    : null    \r\n                    }\r\n                </div>\r\n                {props.question ?\r\n                 <TextBlock text={props.question} /> :\r\n                 null\r\n                }             \r\n            </div>\r\n            {showPopUp === true ? \r\n                <PopUp \r\n                popUpClass={props.popUpClass} \r\n                class='popUp__message popUp__message--error'\r\n                messages={popUpMessages}/>  \r\n            : null}\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default LogSign\r\n\r\n","C:\\Users\\Pusia\\Desktop\\instagram-clone\\src\\Components\\Button\\FbButton.jsx",[],"C:\\Users\\Pusia\\Desktop\\instagram-clone\\src\\Pages\\Auth\\index.jsx",[],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","severity":1,"message":"83","line":21,"column":7,"nodeType":"84","endLine":21,"endColumn":67,"suggestions":"85"},{"ruleId":"82","severity":1,"message":"86","line":38,"column":8,"nodeType":"84","endLine":38,"endColumn":27,"suggestions":"87"},{"ruleId":"88","severity":1,"message":"89","line":78,"column":48,"nodeType":"90","messageId":"91","endLine":78,"endColumn":49,"suggestions":"92"},{"ruleId":"93","severity":1,"message":"94","line":109,"column":46,"nodeType":"95",